@model IEnumerable<ExamenP1JoseSanchez.Models.JSanchez>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EsdelaUdla)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaNacimeinto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Carrera)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EsdelaUdla)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaNacimeinto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Carrera.NombreCarrera)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdSanchez">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdSanchez">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdSanchez">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
